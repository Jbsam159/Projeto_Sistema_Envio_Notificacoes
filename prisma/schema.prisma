generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notification {
  id        String   @id @default(uuid())
  type      String
  recipient String
  message   String
  createdAt DateTime @default(now())
  status    String   @default("Pendente")
}
